include head.jade
body
  include navbar.jade
  .container-fluid
    .row-fluid
      div.left-nav
        img(src="/img/logo.logger.png")
        img.grip(src="/img/grip.png")
        div.search
          input.searchbox(type="text")
        div.options
          div.options-env
            h3 Environments
            div.options-env-qa
              input.chk-env(type="checkbox", checked="true", data-name="qa")
              | QA 
            div.options-env-staging
              input.chk-env(type="checkbox", checked="true", data-name="staging")
              | Staging          
            div.options-env-demo
              input.chk-env(type="checkbox", checked="true", data-name="demo")
              | Demo
            div.options-env-production
              input.chk-env(type="checkbox", checked="true", data-name="production")
              | Production
  
          div.options-component
            h3 Components
            div.options-component-engine
              input.chk-component(type="checkbox", checked="true", data-name="engine")
              | Engine 
            div.options-component-analytics
              input.chk-component(type="checkbox", checked="true", data-name="analytics")
              | Analytics
            div.options-component-sdk
              input.chk-component(type="checkbox", checked="true", data-name="sdk")
              | SDK
            div.options-component-config
              input.chk-component(type="checkbox", checked="true", data-name="config")
              | Configuration
  
          div.options-level
            h3 Level
            div.options-level-silly
              input.chk-level(type="checkbox", checked="true", data-name="silly")
              | Silly
            div.options-level-debug
              input.chk-level(type="checkbox", checked="true", data-name="debug")
              | Debug          
            div.options-level-info
              input.chk-level(type="checkbox", checked="true", data-name="info")
              | Info
            div.options-level-warn
              input.chk-level(type="checkbox", checked="true", data-name="warn")
              | Warning
            div.options-level-error
              input.chk-level(type="checkbox", checked="true", data-name="error")
              | Error
          div.clear-both
        div.control
          button.pause-lines Pause
          button.clear-lines Clear
      include sidebar.jade
      #content.span11
        .row-fluid
          .span12
            h1 Log
            div#console.console
              a(href="#", id="tail", data-ember-action="54")
                span(class="status")
                label Follow logs
              .clearfix

      #widgets-area.span2.hidden-tablet.hidden-phone
        ul#myTab.nav.tab-menu.nav-tabs
          li.active
            a(href='#charts')
              i.fa-icon-bar-chart
          li
            a(href='#users')
              i.fa-icon-group
          li
            a(href='#messages')
              i.fa-icon-envelope
          li
            a#close-widgets-area(href='#')
              i.fa-icon-signin
        #myTabContent.tab-content
          #charts.tab-pane.active
            .bar-stat
              span.title Account balance
              span.value $19 999,99
              span.chart.green 7,3,2,6,6,3,9,0,1,4
            hr
            .bar-stat
              span.title Clients
              span.value 1278
              span.chart.yellow 8,2,5,4,7,4,7,2,5,9
            hr
            .bar-stat
              span.title Orders
              span.value 5876
              span.chart.blue 2,4,6,8,3,5,9,3,2,2
            hr
            .bar-stat
              span.title Messages
              span.value 673
              span.chart.red 0,3,3,8,3,3,2,7,8,4
            hr
            #cpu-usage
            ul.progress-bars
              li
                span.title Free Memory
                span.percent
                .taskProgress.progressSlim.progressGreen 80
              li
                span.title Free HDD Drive
                span.percent
                .taskProgress.progressSlim.progressYellow 20
              li
                span.title Free SSD Drive
                span.percent
                .taskProgress.progressSlim.progressBlue 57
              li
                span.title Transfer
                span.percent
                .taskProgress.progressSlim.progressRed 45
          #users.tab-pane
            ul.users-list
              li
                a(href='#')
                  span.status.active
                  span.avatar
                    img.img-circle(src='img/avatar.jpg', alt='Avatar')
                  span.name Łukasz Holeczek
              li
                a(href='#')
                  span.status.busy
                  span.avatar
                    img.img-circle(src='img/avatar2.jpg', alt='Avatar')
                  span.name Megan Abott
              li
                a(href='#')
                  span.status.disable
                  span.avatar
                    img.img-circle(src='img/avatar3.jpg', alt='Avatar')
                  span.name Kate Ross
              li
                a(href='#')
                  span.status.active
                  span.avatar
                    img.img-circle(src='img/avatar4.jpg', alt='Avatar')
                  span.name Julie Blank
              li
                a(href='#')
                  span.status
                  span.avatar
                    img.img-circle(src='img/avatar5.jpg', alt='Avatar')
                  span.name Jane Sanders
              li
                a(href='#') View all users
          #messages.tab-pane
            ul.messages-list
              li
                a(href='#')
                  .avatar
                    img.img-circle(src='img/avatar.jpg', alt='Avatar')
                  span.name Łukasz Holeczek
                  span.date 25/6/2013
                  span.title Custom Bootstrap design for new client
              li
                a(href='#')
                  .avatar
                    img.img-circle(src='img/avatar2.jpg', alt='Avatar')
                  span.name Megan Abott
                  span.date 25/6/2013
                  span.title Custom Bootstrap design for new client
              li
                a(href='#')
                  .avatar
                    img.img-circle(src='img/avatar3.jpg', alt='Avatar')
                  span.name Kate Ross
                  span.date 25/6/2013
                  span.title Custom Bootstrap design for new client
              li
                a(href='#')
                  .avatar
                    img.img-circle(src='img/avatar4.jpg', alt='Avatar')
                  span.name Julie Blank
                  span.date 25/6/2013
                  span.title Custom Bootstrap design for new client
              li
                a(href='#')
                  .avatar
                    img.img-circle(src='img/avatar5.jpg', alt='Avatar')
                  span.name Jane Sanders
                  span.date 25/6/2013
                  span.title Custom Bootstrap design for new client
              li
                a(href='#') View all messages
      //
         end: Widgets Area
      a#widgets-area-button.hidden-tablet.hidden-phone.open
        i.fa-icon-reorder
    //
      /fluid-row
    #myModal.modal.hide.fade
      .modal-header
        button.close(type='button', data-dismiss='modal') ×
        h3 Settings
      .modal-body
        p Here settings can be configured...
      .modal-footer
        a.btn(href='#', data-dismiss='modal') Close
        a.btn.btn-primary(href='#') Save changes
    .clearfix
    footer
      p
        span(style='text-align: left; float: left;')
          | © 2013
          a(href='http://bootstrapmaster.com', alt='Twitter Bootstrap Themes') creativeLabs
        span.hidden-phone(style='text-align: right; float: right;')
          | Powered by:
          a(href='http://admintemplates.co', alt='Bootstrap Admin Templates') ACME Dashboard

